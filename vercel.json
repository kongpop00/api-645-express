{
  "version": 2,
  
    "functions": {
      "api/users/**/*.js": {
        "maxDuration": 30
      }
    },
  
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/transports/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
